{
  "schemaVersion": 1,
  "id": "godown",
  "version": "${version}",

  "name": "Go Down",
  "description": "On demand crawling.",
  "authors": [
    "Liquidwarp",
    "Francy-Chan"
  ],
  "contact": {
    "homepage": "https://github.com/Francy-chan/go-down/",
    "issues": "https://github.com/Francy-chan/go-down/issues"
  },

  "license": "MIT",
  "icon": "assets/godown/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.liquidwarpmc.godown.GoDown"
    ],
    "client": [
      "net.liquidwarpmc.godown.GoDownClient"
    ]
  },
  "mixins": [
    "godown.mixins.json"
  ],

  "depends": {
    "java": ">=${java_version}",
    "minecraft": ">=${minecraft_version}",
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_api}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Ko-fi": "https://ko-fi.com/francy_chan"
      }
    }
  }
}